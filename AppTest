package com.sdet;
import org.testing.Assert;
import org.testing.annotatiuons.Test;
public class AppTest{
	
	@Test
	Run|Debug
	public void tesztlogin1()
	{
		App myapp=new App();
		Assert.assertEquals(0,myapp.userLogin("abc","abc123"));
	}
	@Test
	Run|Debug
	public void tesztlogin2()
	{
		App myapp=new App();
		Assert.assertEquals(1,myapp.userLogin("abc","abc@123"));
	}
	
	}
